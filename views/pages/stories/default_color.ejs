                </div>
            </div>
        </div>
        <form class="medium-3 large-3 cell icons form_color" action="/story/color/<%= story.id %>"
              method="post"
              style="background-image: linear-gradient(to bottom, <%= type.COLOR_TYPE.color1.topColor %>, <%= type.COLOR_TYPE.color1.bottomColor %>);">
            <input type="text" name="top" id="top" value="<%= type.COLOR_TYPE.color1.topColor %>" hidden/>
            <input type="text" name="bottom" id="bottom" value="<%= type.COLOR_TYPE.color1.bottomColor %>" hidden/>
            <input type="hidden" name="projectId" value="<%= projectItem.id %>">
            <img class="card-image-size" src="<%= sticker.get("uri").url() %>" width="200px" height="200px">
            <div style="color: #ffffff !important;font-weight: 800 !important;font-size: 21px"
                 class="story_summary truncate_summary"> <%= story.get("title") %></div>
            <!--<input class="jscolor {onFineChange:'update(this)'}" value="cc66ff">-->

