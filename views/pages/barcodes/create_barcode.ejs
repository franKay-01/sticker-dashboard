<body class="primary-background-color">

<div class="grid-container">
    <!-- Logo -->
    <div class="medium-12 large-12">
        <a href="/landing">
            <img src="/public/assets/images/G_logo.png"
                 srcset="/public/assets/images/G_logo@2x.png 2x,/public/assets/images/G_logo@3x.png 3x" class="logo"
                 id="logo">
        </a>
    </div>
    <!-- End of logo-->

    <span class="medium-12 large-12 welcome_post" id="pack_id"><b>Barcode Generator</b></span>

    <div class="grid-x grid-margin-x" id="opaqueDiv">

        <div class="medium-3 large-3 cell icons">

            <form action="/barcode" method="post">

                <div class="create_new">
                    <input type="text" class="box_2" placeholder="Name of Sticker" required name="barcode_name">

                    <input type="number" class="box_2" placeholder="Number Required" required  name="barcode_amount">

                    <button type="submit" style="margin-right: 0px !important;color: #a46580" class="create">CREATE</button>
                </div>
            </form>
        </div>

        <%- include('../../partials/footer') %>
    </div>
</div>

