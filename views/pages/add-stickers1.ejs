<!--Drag n Drop Form-->
<form id="upform" action="/uploads" enctype="multipart/form-data" method="post">
    <div class="wrapper">
        <div class="drop">
            <div class="cont">
                <i class="fa fa-cloud-upload"></i>
                <div class="tit">
                    Drag & Drop
                </div>
                <div class="desc">
                    your stickers here, or click to browse
                </div>
            </div>
            <output id="list"></output>
            <input id="files" multiple="true" name="im1[]" type="file"/>
        </div>
    </div>
    <button id="upload1" type="submit">UPLOAD STICKER(S)</button>
    <input type="hidden" name="coll_id" style="display: none" value="<%= id %>">
</form>
<button id="clear">CLEAR</button>

<div id="page-mask"></div>
