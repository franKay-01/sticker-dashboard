<body class="primary-background-color">

<div class="grid-container">
    <!-- Logo -->
    <div class="medium-12 large-12">
            <img src="/public/assets/images/G_logo.png"
                 srcset="/public/assets/images/G_logo@2x.png 2x,/public/assets/images/G_logo@3x.png 3x" class="logo"
                 id="logo">
    </div>
    <!-- End of logo-->

    <span class="medium-12 large-12 welcome_post" id="pack_id"><b>Feedback Collection</b></span>

    <div class="grid-x grid-margin-x" id="opaqueDiv">

        <div class="medium-3 large-3 cell icons">

            <form action="/feedback" method="post">

                    <input type="text" class="box_2" placeholder="Name" required name="name">

                    <input type="text" class="box_2" placeholder="Profession" required name="profession">

                    <input type="email" class="box_2" placeholder="Email" required name="email">

                    <input type="number" class="box_2" placeholder="Phone Number" required name="number">

                    <input type="text" class="box_2" placeholder="Social Media Handle" required name="media">

                    <button type="submit" style="margin-right: 0px !important;color: #a46580" class="create">CREATE</button>

            </form>
        </div>

        <%- include('../partials/footer') %>
    </div>
</div>

