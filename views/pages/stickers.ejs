<!-- Logo -->
<div class="row header-top-spacing">

    <div class="columns large-12 small-12">

        <div class="row">

            <div class="columns large-1 small-12">
                <a href="https://cryptic-waters-41617.herokuapp.com">
                    <img class="logo" src="/public/assets/images/logo.png" alt="Cyfa Logo"/>
