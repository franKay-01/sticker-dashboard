<!--form goes here-->
<div class="container1">
    <form id="tform" action="/upload" method="post">
            <div id="left-col">
                <!--new feature for drag n drop-->
                <div class="demo-droppable">
                    <span id="word">Drag files here or click to upload</span>
                    <div class="output"></div>
                </div>
            </div>
            <div id="right-col">
               <input type="text" placeholder="Sticker Name" id="stname" name="stickername" />
                <input type="text" placeholder="Local Name" id="lname" name="localname">
                <input type="text" placeholder="Category e.g funny, jumpy" id="category" name="cat"/>
                <button id="cancel" href="/dashboard">CANCEL</button> <button id="upload" type="submit">UPLOAD</button>
            </div>
        <span id="filename" name="filename">No file chosen</span>

        <!--<div class="small-4 large-4 columns"><img src="/public/assets/images/DXD.jpg"></div>-->
