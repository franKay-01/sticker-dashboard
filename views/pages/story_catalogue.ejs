<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Packs - G-Stickers</title>

    <% include ../partials/fav.ejs %>

    <link rel="stylesheet" href="/public/assets/css/foundation.min.css">

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/css/materialize.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/public/assets/vendor/slick/slick.css"/>
    <link rel="stylesheet" href="/public/assets/css/custom.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,800,900" rel="stylesheet">
    <!--parse js-->
    <script src="https://npmcdn.com/parse/dist/parse.js"></script>
    <script>
        window.onload = function () {
            document.getElementById("text_element").onclick = function () {
                document.getElementById("story_form").submit();
            }
        }
    </script>
</head>

<body class="primary-background-color">

<div class="grid-container">
    <!-- Logo -->
    <div class="medium-12 large-12">
        <a href="/home">
            <img src="/public/assets/images/G_logo.png"
                 srcset="/public/assets/images/G_logo@2x.png 2x,/public/assets/images/G_logo@3x.png 3x" class="logo"
                 id="logo">
        </a>
        <a id="signoutLink" onclick="logoutUser()">LOGOUT</a>

    </div>
    <!-- End of logo-->
    <div id="pageMask"></div>

    <!-- End of form-->
    <span class="medium-12 large-12 welcome_post" id="pack_id"><b>Story Catalogue</b></span>

    <div class="grid-x grid-margin-x" id="opaqueDiv">
        <div class="medium-3 large-3 cell login_card">
            <div class="card_head_position">
                <div class="stats">Menu</div>
                <div><a class="_stickers" href="/home">Home</a></div>
                <div><a class="_categories" href="/pack_collection">Packs</a></div>
                <div><a class="_stickers_packs" href="/review_collection">Reviews</a></div>
                <div><a class="_stickers_packs" href="/story_collection">Stories</a></div>
                <div><a class="_stickers_packs" href="/story_catalogue">Catalogue</a></div>

            </div>
        </div>

        <div class="medium-3 large-3 cell icons" id="text_element">
            <a href="#" id="text_element">
                <i class="fa fa-text-width awesome_font_img" aria-hidden="true"></i>
                <div><p class="pack_name_text">Text</p></div>
            </a>
        </div>
        <div class="medium-3 large-3 cell icons" id="image_element">
            <a href="#" id="image_element">
                <i class="fa fa-file-image-o awesome_font_img" aria-hidden="true"></i>
                <div><p class="pack_name_text">Image</p></div>
            </a>
        </div>
        <div class="medium-3 large-3 cell icons" id="quote_element">
            <a href="#" id="quote_element">
                <i class="fa fa-quote-left awesome_font" aria-hidden="true"></i>&nbsp;&nbsp;
                <i class="fa fa-quote-right awesome_font" aria-hidden="true"></i>
                <div><p class="pack_name_text" style="margin-top: 50px">Quote</p></div>
            </a>
        </div>
        <div class="medium-3 large-3 cell icons" id="sticker_element">
            <a href="#" id="sticker_element">
                <i class="fa fa-picture-o awesome_font_img" aria-hidden="true"></i>
                <div><p class="pack_name_text">Sticker</p></div>
            </a>
        </div>
        <div class="medium-3 large-3 cell icons" id="divider_element">
            <a href="#" id="divider_element">
                <img class="review_tab" src="/public/assets/images/divider.png">
                <div><p class="pack_name_text">Divider</p></div>
            </a>
        </div>

        <%- include('../partials/footer') %>
    </div>
</div>

<!-- Body -->

<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js" type="application/javascript"/>
<!-- Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.7/js/materialize.min.js"></script>
<script type="text/javascript" src="/public/assets/vendor/slick/slick.min.js"></script>
<script type="text/javascript" src="/public/assets/js/custom.js"></script>
<script type="text/javascript" src="/public/assets/js/validate.js"></script>

</body>


</html>