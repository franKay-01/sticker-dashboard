<% if (locals.allProducts) { %>
<% if (allProducts.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% allProducts.forEach(function(item) { %>
<div class="truncate_title"><a class="home_pack_description" href="/product/<%= item.id %>"><%= item.get("name") %></a></div>
<% }); %>
<div><a class="view_all" href="/products">View All</a></div>
<% } %>
<% } else { %>
<p class="none">None</p>
<% } %>






