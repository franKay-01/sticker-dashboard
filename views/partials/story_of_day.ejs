<!-- Cards -->
<% if (stories.length === 0) { %>
<div class="medium-3 large-3 cell icons">
    <center>
        <p class="none"><b>No Stories</b></p>
    </center>
</div>

<% } else { %>
<% stories.forEach(function(item, index) { %>
<div class="medium-3 large-3 cell icons story_card" data-id="<%= item.id %>" data-name="<%= item.get("title") %>">
    <% artworks.forEach(function (sticker, index) {%>
    <% if (sticker.story === item.id){ %>
    <img src="<%= sticker.image %>" class="card-image-size">
    <% } %>
    <% }) %>

    <div class="name_tag"><p class="sticker_name_text"><%= item.get("title") %></p></div>

</div>

<% }); %>
<% } %>




