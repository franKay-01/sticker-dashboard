<% if (locals.allProjects) { %>
<% if (allProjects.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% allProjects.forEach(function(item) { %>
<div class="truncate_title"><a class="home_pack_description" href="/project/edit/<%= item.id %>"><%= item.get("name") %></a></div>
<% }); %>
<div><a class="view_all" href="/projects">View All</a></div>
<% } %>
<% } else { %>
<p class="none">None</p>
<% } %>






