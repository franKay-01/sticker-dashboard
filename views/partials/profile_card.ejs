<!-- Cards -->
<% if (image.length === 0) { %>
<div class="medium-3 large-3 cell icons">
    <img src="/public/assets/images/broken.png" class="gbeja_notxt">
    <div class="name_tag"><p class="sticker_name_text">Broken Link - Reload</p></div>
</div>

<% } else {%>
<% image.forEach(function(item, index) { %>
<% if (item.get("user_image") !== "null"){%>
<div class="medium-3 large-3 cell icons">
    <img src="<%= item.get("user_image").url() %>" class="gbeja_notxt">
</div>
<%} else {%>
<div class="medium-3 large-3 cell icons">
<input type="file" name="profile_picture">
</div>
<%}%>
<% }); %>
<%}%>
