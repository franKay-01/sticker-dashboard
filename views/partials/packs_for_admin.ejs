<!--Display Collections from parse-->
<% if (collection.length === 0 ){%>
<div class="medium-3 large-3 cell icons">

<p class="none">None</p>

</div>
<%}else {%>
<% collection.forEach(function(item, index) { %>

<div class="medium-3 large-3 cell icons">
    <a href="/pack/<%= item.id %>">
        <% if (item.get("artwork") === "undefined"){ %>
        <img style="width: 255px;margin-left: 30px;" class="btn_admin_pack_icon"
             src="<%= item.get("artwork").url() %>">
        <% } else { %>
        <img class="btn_admin_pack_icon" src="<%= item.get("artwork").url() %>">
        <% } %>
        <div><p class="pack_name_text"><%= item.get("name") %></p></div>

    </a>
    <% if (item.get("status") === 1){ %>
    <span style="color: #8e8e07;margin-left: 5px">#In Review</span>
    <span style="color: rebeccapurple; float: right; margin-right: 5px">#Not Approved</span>
    <% } else if (item.get("status") === 2){ %>
    <span style="color: #8e8e07;margin-left: 5px">#Approved</span>
    <% } %>
</div>

<% }); %>
<%}%>