<!--Display Collections from parse-->

<% collections.forEach(function(item, index) { %>

<div class="medium-3 large-3 cell icons">
    <a href="/pack/<%= item.id %>">
        <img class="btn_pack_icon" src="/public/assets/images/btn-pack-icon.png">
        <div><p class="pack_name_text"><%= item.get("pack_name") %></p></div>
    </a>
</div>

<% }); %>
