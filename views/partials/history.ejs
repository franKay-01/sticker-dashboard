<!-- Cards -->
<% if (items.length === 0) { %>
<div class="medium-3 large-3 cell icons">
    <center>
        <p class="none" style="margin: 145px 90px auto;"><b>No Stickers</b></p>
    </center>
</div>

<% } else { %>
<% items.forEach(function(item, index) { %>

<% if (item.get("type") === type.FEED_TYPE.sticker){%>
<div class="medium-3 large-3 cell card-zoom icons ">
    <center>
        <p><%= date[index] %></p>
    </center>
    <center>
        <img class="pack_artwork" src="<%= item.get("uri").url() %>" style="height: 250px">
    </center>
    <div class="name_tag"><p class="pack_name_text"><%= item.get("name") %></p></div>

</div>
<%} else if (item.get("type") === type.FEED_TYPE.story){%>
<div class="medium-3 large-3 cell card-zoom icons ">
    <center>
        <p><%= date[index] %></p>
    </center>
    <center>
        <img class="pack_artwork" src="<%= item.get("uri").url() %>" style="height: 250px">
    </center>
    <div class="name_tag"><p class="pack_name_text"><%= item.get("name") %></p></div>

</div>
<%}%>
<% }); %>
<% } %>




