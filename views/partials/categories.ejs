
        <% categories.forEach(function(item, index) { %>
        <div class="medium-3 large-3 cell Icons">
            <input type="text" id="categoryname" value="<%= item.get("name")%>" disabled>
                <button class="editbtn1" value="<%= item.id %>">EDIT</button>
                <button class="delbtn1" data-id="<%= item.id %>"  value="<%= item.get("name") %>">REMOVE</button>
        </div>
        <% }); %>
 