<% if (allAdverts.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% allAdverts.forEach(function(item) { %>
<div class="truncate_title"><a class="home_pack_description" href="/advert_details/<%= item.id %>"><%= item.get("title") %></a></div>
<% }); %>
<div><a class="view_all" href="/advert_collection">View All</a></div>
<% } %>






