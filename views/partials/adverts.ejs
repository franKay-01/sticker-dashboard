<% if (locals.allAdverts) { %>
<% if (allAdverts.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% allAdverts.forEach(function(item) { %>
<div class="truncate_title">
    <a class="home_pack_description" href="/advert/edit/<%= item.id %>/<%= projectItem.id %>"><%= item.get("title") %></a>
</div>
<% }); %>
<div><a class="view_all" href="/adverts/<%= projectItem.id %>">View All</a></div>
<% } %>
<% } else { %>
<p class="none">None</p>
<% } %>






