<% if (locals.messages) { %>
<% if (messages.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% messages.forEach(function(item) { %>
<div class="truncate_title"><a class="home_pack_description" href="/message/<%= item.id %>"><%= item.get("email") %></a></div>
<% }); %>
<div><a class="view_all" href="/messages">View All</a></div>
<% } %>
<% } else { %>
<p class="none">None</p>
<% } %>






