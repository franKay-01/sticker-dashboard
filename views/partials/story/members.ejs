<!--Display Collections from parse-->
<%if ( members.length === 0 ){%>
<div class="medium-3 large-3 cell icons">
    <p class="none">No Members</p>
    <div class="name_tag"><p class="pack_name_text">No Members</p></div>

</div>
<%}else {%>
<% members.forEach(function(item, index) { %>

<div class="medium-3 large-3 cell card-zoom icons ">
    <a href="/member/<%= item.id %>/<%= projectItem.id %>">
        <% if (item.get("profileImage") === "undefined" || item.get("profileImage") === undefined){ %>
        <center>
            <p class="none"><b>No Artwork</b></p>
        </center>
        <div class="name_tag"><p class="pack_name_text"><%= item.get("profile").content.name %></p></div>
        <% } else {%>
        <center>
            <img class="pack_artwork" src="<%= item.get("profileImage").url() %>" style="height: 250px">
        </center>
        <div class="name_tag"><p class="pack_name_text"><%= item.get("profile").content.name %></p></div>
        <%}%>
    </a>
</div>

<% }); %>
<%}%>