<% if (locals.story) { %>
<% if (story.length === 0) { %>
<p class="none">None</p>
<% } else { %>
<% story.forEach(function(item) { %>
<% if (item.get("storyType") === type.STORY_TYPE.story || item.get("storyType") === type.STORY_TYPE.short_stories
|| item.get("storyType") === type.STORY_TYPE.chat_group || item.get("storyType") === type.STORY_TYPE.chat_single
|| item.get("storyType") === type.STORY_TYPE.chat_group_episode || item.get("storyType") === type.STORY_TYPE.chat_single_episode){%>
<div class="truncate_title"><a class="home_pack_description" href="/storyedit/<%= item.id %>/<%= projectItem.id %>"><%= item.get("title") %></a></div>
<%}%>
<% }); %>
<div><a class="view_all" href="/stories/<%= projectItem.id %>">View All</a></div>
<% } %>
<% } else { %>
<p class="none">None</p>
<% } %>
