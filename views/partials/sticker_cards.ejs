<!-- Cards -->
<% if (stickers.length === 0) { %>
<div class="medium-3 large-3 cell icons">
    <center>
        <p class="none" style="margin: 145px 90px auto;"><b>No Stickers</b></p>
    </center>
</div>

<% } else { %>
<% stickers.forEach(function(item, index) { %>

<div class="medium-3 large-3 cell card-zoom icons">
    <a href="/sticker/edit/<%= item.id %>/<%= id %>">
        <img src="<%= item.get("uri").url() %>" class="card-image-size">
        <div class="name_tag"><p class="sticker_name_text"><%= item.get("name") %></p></div>
    </a>
    <% if (pack_type === type.PACK_TYPE.curated){%>
    <a href="/pack/stickers/remove/<%= item.id %>/<%= id %>">
        <button class="create_button add_catalogue" style="background: transparent !important;margin-top: 22px !important;position: absolute;">
            <p class="title" style="color:#af627f !important;font-weight: 800 !important;"> Remove</p>
        </button>
    </a>
    <%}%>
</div>

<% }); %>
<% } %>

    
 
    
