<!--Display Collections from parse-->
<%if ( projects.length === 0 ){%>
<div class="medium-3 large-3 cell icons">
    <p class="none">No Projects</p>
    <div class="name_tag"><p class="pack_name_text">No Projects</p></div>

</div>
<%}else {%>
<% projects.forEach(function(item, index) { %>

<div class="medium-3 large-3 cell card-zoom icons ">
    <a href="/home/<%= item.id %>">
        <% if (item.get("uri") === "undefined" || item.get("uri") === undefined){ %>
        <center>
            <p class="none"><b>No Artwork</b></p>
        </center>
        <div class="name_tag" style="background-color: transparent !important;"><p class="pack_name_text"><%= item.get("name") %> Project</p></div>
        <% } else {%>
        <center>
            <img class="pack_artwork" src="<%= item.get("uri").url() %>" style="height: 250px">
        </center>
        <div class="name_tag" style="background-color: transparent !important;"><p class="pack_name_text"><%= item.get("name") %> Project</p></div>
        <%}%>
    </a>

</div>

<% }); %>
<%}%>
